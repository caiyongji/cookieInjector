{
"manifest_version": 3,

"name": "Cookie Injector",
"description": "Cookie Injector: A cookie hacker tools --- inject cookies and export/import cookies as files",
"version": "1.0.11",
"homepage_url": "https://blog.caiyongji.com",
"icons" : {
    "128" : "/icon.png" ,
    "16" : "/icon.png" ,
    "48" : "/icon.png"
  } ,
"action": {
"default_icon": "icon.png",
"default_popup": "popup.html",
"default_title": "Cookie Injector"
},
"background": {
  "service_worker": "background.js",
  "type": "module"
},
"permissions": [
"tabs",
"cookies",
"storage"
],
"host_permissions": [
"http://*/*",
"https://*/*"
],
"content_security_policy": {
"extension_pages": "script-src 'self'; object-src 'self'"
}
}
